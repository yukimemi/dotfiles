[user]
	name = yukimemi
	email = yukimemi@gmail.com
[github]
	user = yukimemi
[push]
	default = simple
[color]
	diff = auto
[alias]
	ci = commit
	co = checkout
	st = status
	a = add
	br = branch
	di = diff
	k = log --graph --pretty
	ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
[core]
	editor = nvim
	autocrlf = false
[ghq]
	root = /Users/yukimemi/.ghq/src
[credential]
	helper = store
[merge]
	tool = vimdiff
[branch]
	autosetuprebase = always
[commit]
	template = /Users/yukimemi/.ghq/src/gist.github.com/nownabe/85d8595fd142adc426fbfaf2bab015e0/.commit_template
[fetch]
	prune = true
