# top-most EditorConfig file
root = true

[*]
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8

[*.go]
indent_style = tab
indent_size = 4

[*.xml]
indent_style = tab
indent_size = 4

[*.bat]
charset = cp932
end_of_line = crlf

[*.ps1]
end_of_line = crlf
