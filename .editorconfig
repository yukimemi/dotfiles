# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.go]
indent_style = tab
indent_size = 4

[*.xml]
indent_style = tab
indent_size = 4

[*.bat]
charset = cp932
end_of_line = crlf

[*.ps1]
end_of_line = crlf
