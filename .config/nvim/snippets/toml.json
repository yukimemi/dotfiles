{
  "plugin": {
    "prefix": ["plugin"],
    "body": [
      "[[plugins]]",
      "repo = '${1:$TM_SELECTED_TEXT}'",
      "if = 0",
      "on_event = ['CursorHold', 'FocusLost']",
      "hook_add = '''",
      "source \\$VIM_PATH/rc/${2:$TM_SELECTED_TEXT}.vim",
      "'''"
    ]
  },
  "header": {
    "prefix": ["header"],
    "body": [
      "# =============================================================================",
      "# File        : ${TM_FILENAME}",
      "# Author      : yukimemi",
      "# Last Change : 2023/09/12 23:30:24.",
      "# =============================================================================",
      ""
    ]
  }
}
