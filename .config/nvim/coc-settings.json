{
  "diagnostic.refreshAfterSave": true,
  "suggest.enablePreview": true,
  "list.source.lists.defaultOptions": ["--no-sort"],
  "list.source.files.args": ["--hidden", "--no-ignore-vcs", "--files"],
  "list.source.grep.defaultArgs": ["-smartcase", "-regex", "-skip-vcs-ignores"],
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "json",
    "html",
    "markdown",
    "rust",
    "vim",
    "html",
    "css",
    "xml"
  ],
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.previewAutoClose": false,
  "translator.toLang": "ja",
  "translator.engines": ["google", "bing"],
  "eslint.autoFixOnSave": true,
  "suggest.asciiCharactersOnly": true,
  "explorer.file.autoReveal": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false
}
