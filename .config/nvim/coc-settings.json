{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.silentAutoupdate": true,
  "coc.preferences.useQuickfixForLocations": true,
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "explorer.file.reveal.auto": true,
  "explorer.file.reveal.whenOpen": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.root.strategies": [
    "reveal",
    "workspace",
    "sourceBuffer"
  ],
  "list.source.files.args": [
    "--hidden",
    "--no-ignore-vcs",
    "--files"
  ],
  "list.source.grep.defaultArgs": [
    "-smartcase",
    "-regex",
    "-skip-vcs-ignores"
  ],
  "list.source.lists.defaultOptions": [
    "--no-sort"
  ],
  "powershell.codeFolding.enable": true,
  "powershell.enableProfileLoading": false,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.codeFormatting.autoCorrectAliases": true,
  "powershell.sideBar.CommandExplorerVisibility": true,
  "npm.binPath": "yarn",
  "suggest.asciiCharactersOnly": true,
  "translator.engines": [
    "google",
    "bing"
  ],
  "translator.toLang": "ja",
  "coc.preferences.formatOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "powershell",
    "ps1",
    "lua",
    "javascript",
    "json",
    "html",
    "rust",
    "vim",
    "html",
    "css",
    "xml"
  ],
  "deno.enable": true,
  "deno.lint": true,
  "coc.source.file.trimSameExts": [],
  "cSpell.enabledLanguageIds": [
    "git-commit",
    "html",
    "markdown",
    "plaintext",
    "restructuredtext",
    "yaml",
    "yml"
  ],
  "mocword.filetypes": [
    "*"
  ],
  "Lua.telemetry.enable": true
}
