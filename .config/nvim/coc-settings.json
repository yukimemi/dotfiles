{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.previewAutoClose": false,
  "coc.preferences.silentAutoupdate": true,
  "coc.preferences.useQuickfixForLocations": true,
  "eslint.autoFixOnSave": true,
  "eslint.run": "onSave",
  "explorer.file.autoReveal": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "list.source.files.args": ["--hidden", "--no-ignore-vcs", "--files"],
  "list.source.grep.defaultArgs": ["-smartcase", "-regex", "-skip-vcs-ignores"],
  "list.source.lists.defaultOptions": ["--no-sort"],
  "markdownlint.onChange": false,
  "markdownlint.onOpen": true,
  "markdownlint.onSave": true,
  "powershell.codeFolding.enable": true,
  "powershell.enableProfileLoading": true,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.codeFormatting.autoCorrectAliases": true,
  "powershell.sideBar.CommandExplorerVisibility": true,
  "npm.binPath": "yarn",
  "suggest.asciiCharactersOnly": true,
  "suggest.enablePreview": true,
  "translator.engines": ["google", "bing"],
  "translator.toLang": "ja",
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "json",
    "html",
    "markdown",
    "rust",
    "vim",
    "html",
    "css",
    "xml"
  ],
  "tsserver.enable": false,
  "deno.enable": true,
  "deno.lint": true,
  "coc.source.file.trimSameExts": [],
  "prettier.disableLanguages": ["typescript", "typescriptreact"],
  "cSpell.enabledLanguageIds": [
    "git-commit",
    "html",
    "json",
    "jsonc",
    "markdown",
    "plaintext",
    "restructuredtext",
    "yaml",
    "yml"
  ]
}
